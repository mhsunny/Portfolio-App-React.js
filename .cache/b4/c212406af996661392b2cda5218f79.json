adventure seeker'\r\n\r\n]\r\n\r\nclass Title extends Component {\r\n\r\n    state = { titleIndex: 0 , fadeIn: true};\r\n\r\n    componentDidMount(){\r\n      this.timeout =  setTimeout(() => this.setState({ fadeIn: false }), 4000);\r\n        this.animateTitles();\r\n    }\r\n    componentWillUnmount() {\r\n        console.log(\"unmount\");\r\n\r\n        clearInterval(this.titleInterval);    \r\n        clearTimeout(this.timeout);\r\n\r\n      }\r\n    animateTitles = () => {\r\n        this.titleInterval = setInterval( () => {\r\n            const titleIndex = (this.state.titleIndex + 1) % TITLES.length; \r\n            this.setState( { titleIndex , fadeIn: true});\r\n            this.timeout =   setTimeout(() => this.setState({ fadeIn: false }), 4000);\r\n\r\n        }, 3000) ;\r\n\r\n        console.log(\"this.titleInterval\", this.titleInterval);\r\n    }\r\n  \r\n    \r\n    render() {\r\n        const {fadeIn, titleIndex } = this.state;\r\n        const title = TITLES[titleIndex];\r\n        return ( \r\n            <h2 className={fadeIn? 'title-fade-in': 'title-fade-out'}> I am { title } </h2>\r\n        )\r\n    }\r\n\r\n}\r\nexport default Title;"},"lineCount":null}},"error":null,"hash":"97f4e52e64633f1fea871409d83dcbb0","cacheData":{"env":{}}}